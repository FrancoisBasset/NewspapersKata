{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [false, "always"],
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "object-literal-sort-keys": false,
        "no-implicit-dependencies": [true, "dev"],
        "no-console": false,
        "no-var-requires": false
    },
    "rulesDirectory": []
}